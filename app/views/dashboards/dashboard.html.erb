<h1>My dashboard</h1>
<div class="container">

<div>
<h2>Profiles:</h2>
<ul>
  <%# iterate through all the profiles %>
  <% @profiles.each do |profile| %>
    <li><%= link_to profile.first_name, profile_path(profile) %></li>
  <% end %>
</ul>
</div>

<div>
  <h2>Events:</h2>
  <ul>
    <%# iterate through all the events %>
    <% @events.each do |event| %>
      <li><%= link_to event.title, event_path(event) %></li>
    <% end %>
  </ul>
</div>
  <!-- Content here -->
</div>
